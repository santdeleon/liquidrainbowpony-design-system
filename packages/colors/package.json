{
  "name": "@liquidrainbowpony/colors",
  "version": "0.2.0",
  "description": "The Liquidrainbowpony Color Palette.",
  "author": "Santiago Deleon <sant@santdeleon.io>",
  "keywords": [
    "@liquidrainbowpony",
    "@liquidrainbowpony/colors",
    "liquidrainbowpony",
    "liquid-rainbow-pony",
    "liquidrainbowpony-design-system",
    "liquidrainbowpony-color-palette"
  ],
  "main": "build/colors.js",
  "style": "build/colors.css",
  "files": [
    "build/*"
  ],
  "scripts": {
    "build": "run-s build:clean build:colors build:style-dictionary",
    "build:clean": "rm -rf ./properties ./build",
    "build:colors": "node \"./figma/index.js\"",
    "build:style-dictionary": "style-dictionary build"
  },
  "publishConfig": {
    "access": "private"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "style-dictionary": "^2.10.2"
  },
  "json": "build/colors.json"
}
