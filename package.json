{
  "author": "Santiago Deleon <sant@santdeleon.io>",
  "name": "@liquidrainbowpony/liquidrainbowpony",
  "version": "0.1.0",
  "description": "The Liquidrainbowpony Design System.",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santdeleon/liquidrainbow.git"
  },
  "keywords": [
    "@liquidrainbowpony",
    "@liquidrainbowpony/core",
    "liquidrainbowpony",
    "liquid-rainbow-pony",
    "liquidrainbowpony-design-system"
  ],
  "scripts": {
    "start": "react-scripts start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.js'",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "bugs": {
    "url": "https://github.com/santdeleon/liquidrainbowpony/issues"
  },
  "homepage": "https://github.com/santdeleon/liquidrainbowpony#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.1",
    "styled-components": "^5.2.1"
  }
}
